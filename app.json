{
  "expo": {
    "name": "SGA Belleza MÃ³vil",
    "slug": "movil-sga-belleza",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/custom-icon.png",
    "scheme": "movilsgabelleza",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.sgabelleza.movil",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/custom-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/custom-icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1e4de5a3-11e2-4e8c-869c-9fedb137e153"
      }
    }
  }
}